# 配置 2.4GHz WiFi (WiFi0) 使用 WiFi6 (AX)
set wireless.radio0.disabled='0'                   # 启用 2.4GHz WiFi
set wireless.radio0.htmode='HE40'                  # 设置频宽为 40MHz（WiFi6）
set wireless.radio0.he_mode='1'                    # 启用 WiFi6 模式
set wireless.radio0.country='CN'                   # 设置国家代码（中国）
set wireless.radio0.channel='auto'                 # 自动选择信道
set wireless.default_radio0.ssid='OpenWrt_2.4GHz' # 设置 2.4GHz SSID
set wireless.default_radio0.encryption='psk2'      # 设置加密方式为 WPA2-PSK
set wireless.default_radio0.key='12345678' # 设置 2.4GHz 密码

# 配置 5GHz WiFi (WiFi1) 使用 WiFi6 (AX)
set wireless.radio1.disabled='0'                   # 启用 5GHz WiFi
set wireless.radio1.htmode='HE160'                 # 设置频宽为 160MHz（WiFi6）
set wireless.radio1.he_mode='1'                    # 启用 WiFi6 模式
set wireless.radio1.country='CN'                   # 设置国家代码（中国）
set wireless.radio1.channel='auto'                 # 自动选择信道
set wireless.default_radio1.ssid='OpenWrt_5GHz'  # 设置 5GHz SSID
set wireless.default_radio1.encryption='psk2'      # 设置加密方式为 WPA2-PSK
set wireless.default_radio1.key='12345678' # 设置 5GHz 密码